{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        // Run whenever any file is saved
        "match": ".*",
        "cmd": "cd ${workspaceFolder}/test/build && cmake .. && make && ctest"
      }
    ]
  },
  "files.associations": {
    "stdbool.h": "c",
    "uism.h": "c",
    "compare": "c",
    "display.h": "c",
    "cmath": "cpp",
    "mock_l_data_int.c": "cpp",
    "mock_r_data_int.c": "cpp",
    "arm_common_tables.h": "c",
    "functional": "c",
    "random": "cpp",
    "ranges": "cpp",
    "array": "cpp",
    "string_view": "cpp",
    "optional": "cpp",
    "ratio": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "variant": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "string": "cpp",
    "deque": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "any": "cpp",
    "numeric": "cpp",
    "chrono": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "unordered_set": "cpp",
    "initializer_list": "cpp",
    "typeinfo": "cpp"
  },
  "arduino.useArduinoCli": true,
  "arduino.logLevel": "info",
  "arduino.allowPDEFiletype": false,
